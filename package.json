  "description": "A re-creation of the popular 2048 game. ",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "node_modules/.bin/webpack build --mode=development",
    "watch-dev": "node_modules/.bin/webpack build --mode=development --watch",
